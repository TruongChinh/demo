var dateOfWeek = ['Chủ nhật','Thứ hai','Thứ ba','Thứ tư','Thứ năm','Thứ sáu','Thứ bảy'];
var date = new Date();
// var fullDateTime = dateOfWeek[date.getDay()]+', '+date.getDate()+'/'+(date.getMonth()+1)+'/'+date.getFullYear() + ' ' + date.getHours()+':' +date.getMinutes() + ' (GMT+7)';
var fullDateTime = dateOfWeek[date.getDay()]+', '+date.getDate()+'/'+(date.getMonth()+1)+'/'+date.getFullYear() + '';
$('#time-now').html(fullDateTime);

var showTime = function(unixtime){
    var onNow = new Date();
    var articleTime = new Date(unixtime*1000);
    var ago = parseInt((onNow.getTime() - articleTime.getTime())/1000);
    var minutes = ago/60;
    var hours = ago/3600;
    if(minutes < 60)
    {
        return '<span class="time-ago" datetime="'+articleTime.getTime()+'">'+Math.ceil(minutes)+"' trước</span>";
    }
    else if(minutes >= 60 && hours <= 24)
    {
        return '<span class="time-ago" datetime="'+articleTime.getTime()+'">'+Math.round(hours)+"h trước</span>";
    }
    else if(hours > 24)
    {
        var currentYear = new Date().getFullYear();
        var articleYear = articleTime.getFullYear();
        if(articleYear === currentYear)
        {
            return articleTime.getDate()+'/'+(articleTime.getMonth()+1);
        }
        else if(currentYear > articleYear)
        {
            return articleTime.getDate()+'/'+(articleTime.getMonth()+1)+'/'+articleTime.getFullYear();
        }
    }
}

var renderComment = function(arrArticle){
    var share_url = arrArticle.share_url.split('?');
    arrArticle.share_url = share_url[0];
	var data = `
        <a class="count_cmt" href="${arrArticle.share_url}#box_comment_vne" style="white-space: nowrap; display: none;">
	     <svg class="ic ic-comment"><use xlink:href="#Comment-Reg"></use></svg>
            <span class="txt_num_comment font_icon" data-type="comment" data-objecttype="${arrArticle.article_type}" data-objectid="${arrArticle.article_id}"></span>
        </a>
        `;
	return data;
}
