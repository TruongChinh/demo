$.get("https://gw.vnexpress.net/bt?site_id=1000000&category_id=1000000&showed_area=trangchu_beta&data_select=title,lead,privacy,thumbnail_url,share_url,article_type,original_cate,article_category,publish_time&thumb_size=300x180&thumb_quality=100&thumb_dpr=1,2&thumb_fit=crop&limit=6", function(data, status){
    var html = '';
    data = data.data['trangchu_beta']['data'];
    for (var i = 0; i < data.length; i++) {
        var lead = '';
        var thumb = '';
        var classOffthumb = '';
        if (data[i].off_thumb == 1 || data[i].thumbnail_url == '') {
            classOffthumb = 'off-thumb';
            var locationName = '';
            if (data[i].location_name != null && data[i].location_name != '') {
                locationName = `<span class="location-stamp">${data[i].location_name}</span>`;
            }
            lead = `<a href="${data[i].share_url}" title="${data[i].title}"><p class="description">${locationName}${data[i].lead}</p></a>`;
        }else{
            var keyImage = 'thumb_300_180_100_1_crop';
            if (data[i].thumbnail_url.includes('.gif')) {
                keyImage = 'thumb_gif_300_180_100_1_crop';
            }
            thumb = `<div class="thumb-art">
                        <a href="${data[i].share_url}" title="${data[i].title}" class="thumb thumb-5x3"> 
                            <img src="${data[i]['thumb_list'][keyImage]}" alt="${data[i].title}">  
                        </a>
                    </div>`;
        }
        html += `<article class="item-news full-thumb ${classOffthumb}">
                    <div class="inner-item-news-five width_common">
                        ${thumb}
                        <h4 class="title-news"><a href="${data[i].share_url}" title="${data[i].title}">${data[i].title}</a></h4>
                        ${lead}
                        <p class="meta-news">
                            <span class="time-public">${showTime(data[i].publish_time)}</span>
                            ${renderComment(data[i])}
                        </p>
                    </div>
                </article>`;
    }
    $('#content-top-news').html(html);

    $.getScript("https://s1.vnecdn.net/vnexpress/restruct/j/v564/v3/production/usi.js",function(){
        if (typeof CmtWidget != 'undefined') {
            CmtWidget.mix();
        }
    });
  });
var header = document.getElementById('wrap-main-nav');
if(header != null)
{
    var topHeader = document.querySelector('.section.top-header');
    var offset = topHeader.offsetTop + topHeader.offsetHeight;
    stickyMenu(offset);
    window.addEventListener('scroll', () => {
        offset = topHeader.offsetTop + topHeader.offsetHeight;
        stickyMenu(offset);
    });
}

function stickyMenu(offset){
    if (window.pageYOffset > offset) {
        if(!header.classList.contains('sticky')){
            header.classList.add('sticky');
            document.querySelectorAll('#wrap-main-nav a[data-itm-source*="vn_campaign=Header"]').forEach(el => {
                var newHref = el.getAttribute('data-itm-source').replace('vn_campaign=Header', 'vn_campaign=Pinbar');
                el.setAttribute('data-itm-source', newHref);
            });
        }
        if(header.classList.contains('show-all-menu')){
            document.querySelector('html').style.overflow = 'hidden';
        }
    } else {
        if(header.classList.contains('sticky')){
            header.classList.remove('sticky');
            document.querySelectorAll('#wrap-main-nav a[data-itm-source*="vn_campaign=Pinbar"]').forEach(el => {
                var newHref = el.getAttribute('data-itm-source').replace('vn_campaign=Pinbar', 'vn_campaign=Header');
                el.setAttribute('data-itm-source', newHref);
            });
        }
        if(!header.classList.contains('show-all-menu')){
            document.querySelector('html').style.overflow = '';
        }
    }
}
